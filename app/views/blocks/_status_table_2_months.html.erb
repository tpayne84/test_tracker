<div class="widget">
  <div class="widget-head">
    <div class="pull-left">Act Now &lt; 2 Months</div>
    <div class="widget-icons pull-right">
      <a href="#" class="wminimize"><i class="icon-chevron-up"></i></a> 
      <a href="#" class="wclose"><i class="icon-remove"></i></a>
    </div>  
    <div class="clearfix"></div>
  </div>
  <div class="widget-content">
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>Expiration</th>
          <th>User</th>
          <th>Company</th>
          <th>Evaluation</th>
        </tr>
      </thead>
      <tbody>
        <% @two_month_report.each do |data| %>
          <tr>
            <td><%= data.requal_date %></td>
            <td><%= data.user.full_name %></td>
            <td><%= data.user.client.name %></td>
            <td><%= @course_name = course_name(data.course_id) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <div class="widget-foot">
      <div class="pagination pull-right">
        <ul>
          <li><a href="#">Prev</a></li>
          <li><a href="#">1</a></li>
          <li><a href="#">2</a></li>
          <li><a href="#">3</a></li>
          <li><a href="#">4</a></li>
          <li><a href="#">Next</a></li>
        </ul>
      </div>
      <div class="clearfix"></div> 
    </div>
  </div>
</div>